{
  "name": "@andersco/mylib",
  "version": "0.0.1",
  "description": "A collection of commonly used utils, components, and more.",
  "scripts": {
    "dev": "pnpm run --if-present --parallel --stream dev",
    "build": "pnpm --stream -r run build",
    "format": "prettier .",
    "format:check": "pnpm format --check",
    "format:fix": "pnpm format --write",
    "lint": "pnpm run --if-present --parallel --stream lint",
    "lint:fix": "pnpm lint --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Anders Ramsay <anders@anders.co> (http://github.com/andersr)",
  "license": "ISC",
  "engines": {
    "node": ">=20.17.0 < 21",
    "pnpm": ">=9.12.3",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@9.12.3",
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  }
}
