{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "module": "esnext",
    "lib": ["esnext"],
    "target": "es6",
    "moduleResolution": "Bundler",
    "strict": true
  },
  "exclude": ["node_modules"]
}
