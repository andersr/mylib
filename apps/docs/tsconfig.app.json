{
  "extends": "@andersco/tsconfig/base.json",
  "include": ["stories", ".storybook"],
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "NodeNext",
    "noEmit": true,
    "target": "es6"
  }
}
